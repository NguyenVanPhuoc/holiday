.dropdown a.dropdown-toggle {
   display: block;
   position: relative;
   padding: 5px 30px 5px 10px;
   color: #333;
   border: 1px solid #c4cdd5;
   border-radius: 3px;
   background: #fff;
}
.dropdown a.dropdown-toggle:before {
   position: absolute;
   right: 10px;
   top: 7px;
   content: "\f0dd";
   font-family: 'FontAwesome';
   color: #c0bdbd;
   font-size: 12px;
}
.dropdown a.dropdown-toggle:after {
   position: absolute;
   right: 10px;
   top: 7px;
   content: "\f0de";
   font-family: 'FontAwesome';
   color: #c0bdbd;
   font-size: 12px;
}
