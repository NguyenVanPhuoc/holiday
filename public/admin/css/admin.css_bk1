nav#menu{
   list-style: none;
}
nav#menu ul{
    padding-left: 0;
}
nav#menu li{
   list-style: none;
   border-left: 4px solid transparent;
}
nav#menu #sidebar .has-children .sub-menu{
   padding-left: 0;
}
nav#menu li.active{
   border-left: 4px solid #19aa8d;
   background: #293846;
}
nav#menu li a{
   display: block;
   color: #a7b1c2;
   padding: 14px 20px 14px 35px;
   position: relative;
}
nav#menu li a i{
   position: absolute;
   left: 14px;
   top: 17px;
   font-size: 13px;
}
nav#menu li a i.fa-home{
   font-size: 14px;
   top: 16px;
}
nav#menu li a:hover, nav#menu li.active a{
    background: #293846;
    color: #fff;
}
nav#menu li.has-children.active a:before{
    content: "\f107"
}
nav#menu li.has-children.active .sub-menu a:before{
    content: none;
}
nav#menu li.has-children a:before{
   position: absolute;
   content: "\f105";
   top: 13px;
   right: 15px;
   font-family: 'FontAwesome';
}
nav#menu li.active .sub-menu a{
    color: #a7b1c2;
}
nav#menu li.active .sub-menu li{
    border-left:none;
}
nav#menu li.active .sub-menu .active a,nav#menu li.active .sub-menu a:hover{
    color: #fff;
}
nav#menu .has-children .sub-menu a{
   padding: 7px 10px 7px 10px;
   padding-left: 35px;
}
nav#menu .sub-menu{
   display: none;
}
/*nav#menu li.active .sub-menu{
    height: 100%;
     opacity: 1
}
*/
nav#menu .active .sub-menu{
    display: block;
}
nav#menu .has-children .sub-menu a:before{
    content: none;
}
